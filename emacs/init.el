;; custom theme
(load-theme 'atom-one-dark t)

;; font setting
(set-frame-font "Monaco 11" nil t)

;; ruler 80
(add-hook 'prog-mode-hook #'display-fill-column-indicator-mode)

;; set C indentation
(setq c-default-style "linux" c-basic-offset 8)
